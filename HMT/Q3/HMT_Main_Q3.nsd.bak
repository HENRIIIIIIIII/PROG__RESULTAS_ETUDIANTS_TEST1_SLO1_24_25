<?xml version="1.0" encoding="UTF-8"?>
<root xmlns:nsd="https://structorizer.fisch.lu" version="3.30-10" preRepeat="until " postFor="to" preReturn="return" postForIn="in" preWhile="while " output="OUTPUT" input="INPUT" preFor="for" preExit="exit" preLeave="leave" ignoreCase="true" preThrow="throw" preForIn="foreach" stepFor="by" author="benschafrot" created="2024-09-05" changedby="henmott" changed="2024-12-13" origin="Structorizer 3.30-10" text="&#34;HMT_Main_Q3&#34;,&#34;&#34;,&#34;Titre: Travail Ecrit 1 - Correction&#34;,&#34;Projet         : gestion de temperature&#34;,&#34;Description    : Test Question 3&#34;,&#34;&#34;,&#34;Nom           : Henri Mott&#34;,&#34;Date           : 31.10.24&#34;,&#34;Lieu           : ETML-ES &#34;" comment="" color="ffffff" type="program" style="nice">
	<children>
		<instruction text="&#34;define:&#34;,&#34;&#34;,&#34;entier, 4bit, TEMPS_MAX = 50           // pour alarme clignote&#34;,&#34;entier, 4bit,  TEMPS_MIN = -10           // alarme activée sans clignotement&#34;,&#34;valeur réel, nonsigné, 8bit TEMPS_DEL_ALA = 30           //différence de température(intérieur-extérieur)&#34;,&#34;valeur réel, nonsigné, 8bit TEMP_HEATER = 18           // Temperature d'intérieur trop froid&#34;,&#34;valeur réel, nonsigné, 8bit TEMPS_VENTI = 25           // Temperature d'intérieur trop chaux&#34;,&#34;valeur réel, nonsigné, 8bit TEMPS_DEL_CHAUF = 10           //différence de température(intérieur-extérieur)&#34;,&#34;valeur réel, nonsigné, 8bit TEMPS_DEL_VEN = 5           //différence de température(intérieur-extérieur)&#34;,&#34;&#34;,&#34;Variable: &#34;,&#34;&#34;,&#34;//stockage des valeurs&#34;,&#34;entier, non signé 4bytes, Temps_Ex = 0&#34;,&#34;entier, non signé 4bytes, Temps_In = 0&#34;" comment="&#34;declaration de variable et define&#34;" color="c0c0c0" rotated="0" disabled="0"></instruction>
		<while text="&#34;Faire toute les minute&#34;" comment="&#34;itération&#34;" color="ffff80" disabled="0">
			<qWhile>
				<call text="&#34;Lecture capteur de température 2&#34;,&#34;Conversions Tension en Degre&#34;" comment="" color="c0c0c0" disabled="0"></call>
				<call text="&#34;Lecture de capteur Temperature 1&#34;,&#34;Conversions valeur Binaire en Degre&#34;" comment="&#34;//envoyer valeur analogique sur un convertisseur analogique à numérique&#34;" color="c0c0c0" disabled="0"></call>
				<alternative text="&#34;//Alarme clignote&#34;,&#34;if(Temps_Ex &#60; TEMPS_MIN)&#34;" comment="&#34;&#34;" color="ffffff" disabled="0">
					<qTrue>
						<call text="&#34;// fonction pour faire basculer l'alarme qui clignote&#34;,&#34;Fonction Alarme clignotante activée&#34;" comment="" color="80ff80" disabled="0"></call>
					</qTrue>
					<qFalse>
						<call text="&#34;// fonction pour désactiver l'alarme qui clignote&#34;,&#34;Fonction Alarme clignotante désactivée&#34;" comment="" color="ff8080" disabled="0"></call>
					</qFalse>
				</alternative>
				<alternative text="&#34;//Alarme&#34;,&#34;if((Temps_Ex - Temps_In) =&#62; TEMPS_DEL_ALA )&#34;" comment="" color="ffffff" disabled="0">
					<qTrue>
						<call text="&#34;// fonction pour faire basculer l'alarme&#34;,&#34;Fonction Alarme activée&#34;" comment="" color="80ff80" disabled="0"></call>
					</qTrue>
					<qFalse>
						<call text="&#34;// fonction pour désactivé l'alarme&#34;,&#34;Fonction Alarme désactivée&#34;" comment="" color="ff8080" disabled="0"></call>
					</qFalse>
				</alternative>
				<alternative text="&#34;//Chauffage&#34;,&#34;if((Temps_In &#60; TEMP_HEATER) ET ((Temps_In - Temps_Ex) &#62; TEMPS_DEL_CHAUF ) ET (Temps_In &#60;= Temps_Ex))&#34;" comment="" color="ffffff" disabled="0">
					<qTrue>
						<call text="&#34;// fonction pour faire activer le chauffage&#34;,&#34;Fonction chauffage activée&#34;" comment="" color="80ff80" disabled="0"></call>
					</qTrue>
					<qFalse>
						<call text="&#34;// fonction pour désactivé chauffage&#34;,&#34;Fonction chauffage désactivée&#34;" comment="" color="ff8080" disabled="0"></call>
					</qFalse>
				</alternative>
				<alternative text="&#34;//Ventilation&#34;,&#34;if((Temps_In &#62; TEMPS_VENTI ) ET ((Temps_In - Temps_Ex) &#62; TEMPS_DEL_VEN ) ET (Temps_In &#62;= Temps_Ex)))&#34;" comment="" color="ffffff" disabled="0">
					<qTrue>
						<call text="&#34;// fonction pour faire activer la Ventilation&#34;,&#34;Fonction Ventilation activée&#34;" comment="" color="80ff80" disabled="0"></call>
					</qTrue>
					<qFalse>
						<call text="&#34;// fonction pour désactivé la Ventilation&#34;,&#34;Fonction Ventilation désactivée&#34;" comment="" color="ff8080" disabled="0"></call>
					</qFalse>
				</alternative>
			</qWhile>
		</while>
	</children>
</root>